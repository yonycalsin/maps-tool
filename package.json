{
  "name": "maps-preview",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "serve": "vite preview",
    "lint": "eslint . --ext .jsx,.js,.ts,.tsx --quiet --ignore-path ./.gitignore",
    "lint:fix": "npm run lint -- --fix",
    "prettier": "prettier --ignore-path .gitignore \"**/*.{js,jsx,json,ts,tsx,scss,css,html,md}\"",
    "check-format": "npm run prettier -- --list-different",
    "format": "npm run prettier -- --write",
    "type-check": "tsc",
    "validate": "run-p check-format lint type-check"
  },
  "dependencies": {
    "react": "^17.0.0",
    "react-dom": "^17.0.0"
  },
  "devDependencies": {
    "@types/node": "^16.4.3",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^4.17.0",
    "@typescript-eslint/parser": "^4.17.0",
    "@vitejs/plugin-react-refresh": "^1.3.1",
    "autoprefixer": "^10.3.1",
    "eslint": "^7.28.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.3.6",
    "postcss-nested": "^5.0.5",
    "pre-commit": "^1.2.2",
    "prettier": "^2.2.1",
    "tailwindcss": "^2.2.7",
    "typescript": "^4.1.2",
    "vite": "^2.1.0"
  },
  "pre-commit": "lint",
  "license": "ISC",
  "author": "Yony Calsin"
}
